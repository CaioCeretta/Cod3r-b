{
  "name": "urna",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:frontend": "cd apps/frontend && npm run dev",
    "dev:backend": "cd apps/backend && npm run start:dev",
    "dev:auth": "cd packages/auth && npm run dev",
    "dev:core": "cd packages/core && npm run dev",
    "dev:shared": "cd packages/shared && npm run dev"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0"
  }
}