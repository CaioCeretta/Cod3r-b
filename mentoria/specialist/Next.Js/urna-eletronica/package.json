{
  "name": "urna",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:frontend": "cd apps/frontend && npm run dev",
    "dev:backend": "cd apps/backend && npm run start:dev",
    "dev:auth": "cd packages/auth && npm run dev",
    "dev:votacao": "cd packages/votacao && npm run dev",
    "dev:shared": "cd packages/shared && npm run dev"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}